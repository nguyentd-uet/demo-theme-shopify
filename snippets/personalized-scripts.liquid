{% if template contains 'product' %}
  {% assign isPersonalized = false %}
  {% assign vendor = product.vendor | downcase %}
  {% if vendor == 'customall' %}
    <script>
      window.money_format = "{{ shop.money_format }}";window.pbstoreid="[[.STORE_ID]]";
    </script>
    <script>
      const productType = "{{ product.type }}";
      if (productType == 'Acrylic Keychain' || productType == 'Wooden Keychain') {
        window.markBestSeller = {
          option: 'Package',
          value: 'Set Of 2',
          text: 'Popular',
        }
      }
    </script>
    <style>
      form[action='/cart/add'] [data-shopify='payment-button'],
      form[action='/cart/add'] .shopify-payment-button{
        display: none !important;
      }
    </style>
	<link
      rel="stylesheet"
      type="text/css"
      href="https://apis-v2.customall.io/alpha/style.css"
      as="style"
    />
	<link
      rel="preload"
      href="https://apis-v2.customall.io/alpha/{{ shop.permanent_domain }}/{{ product.id }}"
      as="script"
    />
	<script
      type="text/javascript"
      src="https://apis-v2.customall.io/alpha/{{ shop.permanent_domain }}/{{ product.id }}"
    ></script>
  {% endif %}
{% endif %}
<style>.product-details__item--property{display:none}</style>